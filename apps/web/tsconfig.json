{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components/*": [
        "./components/*"
      ],
      "@/lib/*": [
        "./lib/*"
      ],
      "@/types/*": [
        "./types/*"
      ],
      "@/hooks/*": [
        "./hooks/*"
      ],
      "@/utils/*": [
        "./lib/*",
        "./utils/*"
      ],
      "@/styles/*": [
        "./app/*",
        "./styles/*"
      ],
      "@/data/*": [
        "./data/*"
      ],
      "@/stores/*": [
        "./stores/*"
      ]
    }
  },
  "include": [
    "app/**/*.ts",
    "app/**/*.tsx",
    "components/**/*.ts",
    "components/**/*.tsx",
    "lib/**/*.ts",
    "lib/**/*.tsx",
    "stores/**/*.ts",
    "stores/**/*.tsx",
    "shims/**/*.ts",
    "shims/**/*.tsx",
    "types/**/*.ts",
    ".next/types/**/*.ts"
  ]
}
