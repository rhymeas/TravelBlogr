{
  "name": "travelblogr",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev apps/web",
    "build": "next build apps/web",
    "start": "next start apps/web",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > infrastructure/database/types.ts",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "crawler:install": "npm install crawlee playwright cheerio axios",
    "crawler:setup": "npx playwright install chromium",
    "generate-blog-posts": "tsx scripts/generate-trip-blog-posts.ts",
    "enrich-activities": "tsx scripts/enrich-location-activities.ts",
    "enrich-activities:limit": "tsx scripts/enrich-location-activities.ts --limit",
    "enrich-activities:location": "tsx scripts/enrich-location-activities.ts --location-id",
    "enhance-trips": "tsx scripts/enhance-existing-trips.ts",
    "enhance-trips:dry-run": "tsx scripts/enhance-existing-trips.ts --dry-run",
    "cache:list": "tsx scripts/upstash-cache-inspector.ts list",
    "cache:get": "tsx scripts/upstash-cache-inspector.ts get",
    "cache:delete": "tsx scripts/upstash-cache-inspector.ts delete",
    "cache:clear": "tsx scripts/upstash-cache-inspector.ts clear",
    "cache:stats": "tsx scripts/upstash-cache-inspector.ts stats",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.0.2",
    "@react-email/components": "^0.5.6",
    "@react-email/render": "^1.3.2",
    "@stripe/stripe-js": "^8.1.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.38.4",
    "@tremor/react": "^3.18.7",
    "@types/qrcode": "^1.5.5",
    "@types/react-image-gallery": "^1.2.4",
    "@uploadthing/react": "^6.0.2",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "browser-image-compression": "^2.0.2",
    "cheerio": "^1.0.0-rc.12",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "compressorjs": "^1.2.1",
    "crawlee": "^3.7.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.16",
    "fuse.js": "^7.0.0",
    "groq-sdk": "^0.3.2",
    "immer": "^10.0.3",
    "lucide-react": "^0.294.0",
    "maplibre-gl": "^3.6.2",
    "nanoid": "^5.0.4",
    "next": "14.0.4",
    "novel": "^1.0.2",
    "nprogress": "^0.2.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-comments-section": "^3.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.1",
    "react-image-gallery": "^1.4.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-intersection-observer": "^9.5.3",
    "react-photo-album": "^2.3.0",
    "resend": "^6.2.2",
    "stripe": "^19.1.0",
    "swiper": "^11.0.5",
    "swr": "^2.2.4",
    "tailwind-merge": "^2.0.0",
    "uploadthing": "^6.0.4",
    "uuid": "^13.0.0",
    "yet-another-react-lightbox": "^3.25.0",
    "zod": "^4.1.12",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.20",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@upstash/cli": "^0.3.0",
    "autoprefixer": "^10.4.16",
    "dotenv": "^17.2.3",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.4",
    "inversify": "^7.10.2",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.3.5",
    "to-ico": "^1.1.5",
    "tsx": "^4.20.6",
    "typescript": "^5.3.2"
  }
}
